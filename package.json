{
   "name": "discordbot",
   "version": "1.0.0",
   "main": "build/main.js",
   "author": "Giant_T",
   "license": "MIT",
   "type": "module",
   "scripts": {
      "start": "ts-node src/index.ts",
      "prestart": "tsc --build",
      "dev": "npx tsx src/main.ts",
      "fmt": "yarn prettier --write src/",
      "build": "tsc --build",
      "clean": "tsc --build --clean",
      "prepare": "husky install",
      "typeorm": "typeorm-ts-node-commonjs"
   },
   "lint-staged": {
      "src/**/*": "prettier --write --ignore-unknown src/"
   },
   "devDependencies": {
      "@faker-js/faker": "^8.0.1",
      "@types/node": "^16.11.10",
      "@typescript-eslint/parser": "^5.59.5",
      "eslint": "^8.40.0",
      "eslint-config-prettier": "^8.8.0",
      "husky": "^8.0.3",
      "lint-staged": "^13.2.2",
      "prettier": "^2.8.8",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "axios": "^1.4.0",
      "discord.js": "^14.11.0",
      "dotenv": "^16.0.3",
      "mongodb": "^5.2.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.17",
      "pg": "^8.4.0"
   }
}