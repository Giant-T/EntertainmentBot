{
  "name": "discordbot",
  "version": "1.0.0",
  "main": "build/main.js",
  "author": "Giant_T",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "tsc --build && node .",
    "fmt": "yarn prettier --write src/",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*": "prettier --write --ignore-unknown src/"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.1",
    "@types/node": "^20.1.3",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "discord.js": "^14.11.0",
    "dotenv": "^16.0.3"
  }
}
